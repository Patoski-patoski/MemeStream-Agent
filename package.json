    "body-parser": "^2.2.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "node-telegram-bot-api": "^0.66.0",
    "playwright": "^1.53.2",
    "typescript": "^5.8.3",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/express": "^5.0.3",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
