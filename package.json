{
  "name": "gemini-ts-app",
  "version": "1.0.0",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "dev": "nodemon",
    "build": "tsc",
    "lint": "eslint './src/**/*.ts' './tests/**/*.ts' --fix -f stylish",
    "start": "node dist/bot/bot.js",
    "prebuild": "rm -rf dist",
    "docker:build": "docker build -t memestream-agent .",
    "docker:run": "docker run --env-file .env -p 3300:3300 memestream-agent",
    "docker:dev": "docker run --env-file .env -p 3300:3300 -v $(pwd)/src:/app/src memestream-agent npm run dev"
  },
